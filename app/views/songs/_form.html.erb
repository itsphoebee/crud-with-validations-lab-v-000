<%= form_for @song do |s| %>
  <%= s.text_field :title, :value => @song.title %>
  <%= s.text_field :released, :value => @song.released%>
  <%= s.text_field :release_year, :value => @song.release_year%>
  <%= s.text_field :artist_name, :value => @song.artist_name %>
  <%= s.text_field :genre, :value => @song.genre %>
  <%= s.submit 'Save' %>
<% end %>
