<%= form_for @song do |s| %>
  <%= s.label :title %>:<br>
  <%= s.text_field :title, :value => @song.title %><br>

  <%= s.label :released %>:<br>
  <%= s.text_field :released, :value => @song.released%><br>

  <%= s.label :release_year %>:<br>
  <%= s.text_field :release_year, :value => @song.release_year%><br>

  <%= s.label :artist_name %>:<br>
  <%= s.text_field :artist_name, :value => @song.artist_name %><br>

  <%= s.label :genre %>:<br>
  <%= s.text_field :genre, :value => @song.genre %><br>

  <%= s.submit 'Save' %>
<% end %>
